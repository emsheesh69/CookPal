<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background Color -->
    <solid android:color="#422020" />

    <!-- Border Color and Thickness -->
    <stroke
        android:width="1dp"
        android:color="#7A5E5E" />

    <!-- Rounded Corners -->
    <corners android:radius="12dp" />

    <!-- Padding inside the item -->
    <padding
        android:left="8dp"
        android:top="8dp"
        android:right="8dp"
        android:bottom="8dp" />
</shape>